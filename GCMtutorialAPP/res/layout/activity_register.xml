<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.gcmtutorial.MainActivity"
    tools:ignore="MergeRootFrame" >

    <Button
        android:id="@+id/btnGCMRegister"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="84dp"
        android:text="Register to GCM Server" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/btnGCMRegister"
        android:layout_alignParentTop="true"
        android:layout_marginTop="24dp"
        android:text="GCM DEMO APP"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/btnGCMRegister"
        android:layout_below="@+id/btnGCMRegister"
        android:layout_marginLeft="14dp"
        android:layout_marginTop="51dp"
        android:text="Tech-Papers"
        android:textAppearance="?android:attr/textAppearanceLarge" />

</RelativeLayout>